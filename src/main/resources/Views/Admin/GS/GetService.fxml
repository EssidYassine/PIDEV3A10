            </GridPane>
        </ScrollPane>
      <Button layoutX="20.0" layoutY="15.0" mnemonicParsing="false" onAction="#revenirFenetreGestion1" prefHeight="38.0" prefWidth="77.0">
         <graphic>
            <ImageView fitHeight="31.0" fitWidth="66.0" pickOnBounds="true" preserveRatio="true">
