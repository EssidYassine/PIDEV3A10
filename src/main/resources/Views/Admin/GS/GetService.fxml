            <GridPane fx:id="gridPane" hgap="20" style="-fx-background-color: #f4f4f4; -fx-padding: 15px; -fx-border-radius: 10;" vgap="20">
                <columnConstraints>
                    <ColumnConstraints prefWidth="200.0" />
