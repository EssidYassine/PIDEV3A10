<AnchorPane prefHeight="700.0" prefWidth="900.0"  stylesheets="@styles.css" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Controllers.Admin.GS.GetServiceController">

